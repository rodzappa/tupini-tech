{
  "name": "tupini-tech",
  "description": "General Purpose Angular Utilities Library",
  "version": "0.0.1",
  "homepage": "https://github.com/rodzappa/tupini-tech",
  "author": "Rodolfo Zappa (https://github.com/rodzappa)",
  "license": "MIT",
  "keywords": [
    "cpf",
    "cnpj",
    "angular",
    "validator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodzappa/tupini-tech.git"
  },
  "bugs": {
    "url": "https://github.com/rodzappa/tupini-tech/issues"
  },
  "private": false,
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "add-git:tupiniquim-validators": "git add libs/tupiniquim-validators/package.json",
    "add-git": "run-p add-git:tupiniquim-validators",
    "commit-git": "run-s add-git && git commit -m \"build: adds new release\" && git push origin && git push origin --tags",
    "pre-publish:minor": "run-s release:minor sync-version build:lib-pack commit-git",
    "pre-publish:major": "run-s release:major sync-version build:lib-pack commit-git",
    "pre-publish:patch": "run-s release:patch sync-version build:lib-pack commit-git",
    "sync-version": "run-p sync-version:tupiniquim-validators",
    "sync-version:tupiniquim-validators": "sync-json -v --property version --source package.json libs/tupiniquim-validators/package.json",
    "copy-license:tupiniquim-validators": "cp LICENSE dist/libs/tupiniquim-validators",
    "copy-changelog:tupiniquim-validators": "cp CHANGELOG.md dist/libs/tupiniquim-validators",
    "copy-files:tupiniquim-validators": "run-p copy-license:tupiniquim-validators copy-changelog:tupiniquim-validators",
    "build:lib-pack:tupiniquim-validators": "nx build --prod && run-s copy-files:tupiniquim-validators && cd dist/libs/tupiniquim-validators && npm pack",
    "build:lib-pack": "run-p build:lib-pack:tupiniquim-validators"
  },
  "dependencies": {
    "@angular/animations": "^10.1.0",
    "@angular/common": "^10.1.0",
    "@angular/compiler": "^10.1.0",
    "@angular/core": "^10.1.0",
    "@angular/forms": "^10.1.0",
    "@angular/platform-browser": "^10.1.0",
    "@angular/platform-browser-dynamic": "^10.1.0",
    "@angular/router": "^10.1.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/compiler-cli": "^10.1.0",
    "@angular/language-service": "^10.1.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nrwl/angular": "^10.4.4",
    "@nrwl/cli": "10.4.4",
    "@nrwl/eslint-plugin-nx": "10.4.4",
    "@nrwl/jest": "10.4.4",
    "@nrwl/tao": "10.4.4",
    "@nrwl/workspace": "10.4.4",
    "@types/jest": "26.0.8",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "~5.0.1",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "husky": "^4.3.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "ng-packagr": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.2",
    "standard-version": "^9.0.0",
    "sync-json": "^1.0.2",
    "ts-jest": "26.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~4.0.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
